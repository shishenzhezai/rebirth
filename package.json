    "@angular/forms": ">=4.0.0-rc.3 <5.0.0",
    "@angular/http": ">=4.0.0-rc.3 <5.0.0",
    "@angular/platform-browser": ">=4.0.0-rc.3 <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0-rc.3 <5.0.0",
    "@angular/router": ">=4.0.0-rc.3 <5.0.0",
    "bootstrap-sass": "^3.3.7",
    "classlist.js": "^1.1.20150312",
    "codemirror": "^5.21.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "highlight.js": "^9.6.0",
    "ie-shim": "^0.1.0",
    "immutable": "^3.8.1",
    "lodash": "^4.0.0-rc.1",
    "markdown-it": "^8.2.1",
    "normalize.css": "^5.0.0",
    "rebirth-event-source": "0.0.1",
    "rebirth-http": "^0.0.9",
    "rebirth-permission": "^0.0.4",
    "rebirth-storage": "^0.0.7",
    "rxjs": "^5.1.0",
